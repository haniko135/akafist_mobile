        android:id="@+id/starredFragment"
        android:name="net.energogroup.akafist.fragments.StarredFragment"
        android:label="fragment_starred"
        tools:layout="@layout/fragment_starred" >
        <action
            android:id="@+id/action_starredFragment_to_prayerFragment"
            app:destination="@id/prayerFragment" />
    </fragment>
    <fragment
        android:id="@+id/prayerRuleFragment"
        android:name="net.energogroup.akafist.fragments.PrayerRuleFragment"
        android:label="fragment_prayer_rule"
        tools:layout="@layout/fragment_prayer_rule" >
        <action
            android:id="@+id/action_prayerRuleFragment_to_prayerFragment"
            app:destination="@id/prayerFragment" />
    </fragment>
    <fragment
        android:id="@+id/calendarFragment"
        android:name="net.energogroup.akafist.fragments.CalendarFragment"
        android:label="fragment_calendar"
        tools:layout="@layout/fragment_calendar" />
    <action
        android:id="@+id/action_global_calendarFragment"
        app:destination="@id/calendarFragment"
        app:enterAnim="@anim/enter_left"
        app:exitAnim="@anim/exit_right"
        app:popEnterAnim="@anim/enter_right"
        app:popExitAnim="@anim/exit_left" />
</navigation>
        <action
            android:id="@+id/action_churchFragment_to_prayerFragment"
            app:destination="@id/prayerFragment"
            app:enterAnim="@anim/enter_left"
            app:exitAnim="@anim/exit_right"
            app:popEnterAnim="@anim/enter_right"
            app:popExitAnim="@anim/exit_left" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="net.energogroup.akafist.fragments.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_home2"
            app:destination="@id/home2"
            app:enterAnim="@anim/enter_left"
            app:exitAnim="@anim/exit_right"
            app:popEnterAnim="@anim/enter_right"
            app:popExitAnim="@anim/exit_left" />
    </fragment>
    <action
        android:id="@+id/action_global_loginFragment"
        app:destination="@id/loginFragment"
        app:enterAnim="@anim/enter_left"
        app:exitAnim="@anim/exit_right"
        app:popEnterAnim="@anim/enter_right"
        app:popExitAnim="@anim/exit_left" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="net.energogroup.akafist.fragments.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_settingsFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/enter_left"
            app:exitAnim="@anim/exit_right"
            app:popEnterAnim="@anim/enter_right"
            app:popExitAnim="@anim/exit_left" />
        <action
            android:id="@+id/action_settingsFragment_to_accountFragment"
            app:destination="@id/accountFragment"
            app:enterAnim="@anim/enter_left"
            app:exitAnim="@anim/exit_right"
            app:popEnterAnim="@anim/enter_right"
            app:popExitAnim="@anim/exit_left" />
    </fragment>
    <action
        android:id="@+id/action_global_settingsFragment"
        app:destination="@id/settingsFragment"
        app:enterAnim="@anim/enter_left"
        app:exitAnim="@anim/exit_right"
        app:popEnterAnim="@anim/enter_right"
        app:popExitAnim="@anim/exit_left" />
    <fragment
        android:id="@+id/accountFragment"
        android:name="net.energogroup.akafist.fragments.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" >
        <action
            android:id="@+id/action_accountFragment_to_starredFragment"
            app:destination="@id/starredFragment" />
        <action
            android:id="@+id/action_accountFragment_to_prayerRuleFragment"
            app:destination="@id/prayerRuleFragment" />
        <action
            android:id="@+id/action_accountFragment_to_calendarFragment"
            app:destination="@id/calendarFragment" />
    </fragment>
    <action
        android:id="@+id/action_global_accountFragment"
        app:destination="@id/accountFragment"
        app:enterAnim="@anim/enter_left"
        app:exitAnim="@anim/exit_right"
        app:popEnterAnim="@anim/enter_right"
        app:popExitAnim="@anim/exit_left" />
